        "# Number Guessing Game : With Latency Calculation\n",
        "import time\n",
        "import random\n",
        "\n",
        "start = time.perf_counter()\n",
        "\n",
        "number = random.randint(1,5)\n",
        "print(number)\n",
        "\n",
        "if number == 1:\n",
        "  print(\"Number is 1\")\n",
        "elif number == 2:\n",
        "  print(\"Number is 2\")\n",
        "elif number == 3:\n",
        "  print(\"Number is 3\")\n",
        "elif number == 4:\n",
        "  print(\"Number is 4\")\n",
        "elif number == 5:\n",
        "  print(\"Number is 5\")\n",
        "else:\n",
        "  print(\"No Number\")\n",
        "\n",
        "# Your code that you want to time\n",
        "end = time.perf_counter() - start\n",
        "print('{:.6f}s for the calculation'.format(end))"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "dX15ydkWJnZf",
        "outputId": "8923f7d7-427a-4134-f3e2-a6e4ea535f95"
      },
      "execution_count": 19,
