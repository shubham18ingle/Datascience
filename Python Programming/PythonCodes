        "even = []\n",
        "odd  = []\n",
        "\n",
        "def compute():  # function definition\n",
        "\n",
        "  for i in range(1,30):\n",
        "\n",
        "    if i %2 == 0:\n",
        "      even.append(i)\n",
        "    else:\n",
        "      odd.append(i)\n",
        "\n",
        "compute()  # function calling\n",
        "\n",
        "print(even)\n",
        "print(odd)"
      ],
      "metadata": {
        "colab": {
