      "cell_type": "code",
      "source": [
        "# Number Guessing Game\n",
        "import time\n",
        "import random\n",
        "\n",
        "start = time.perf_counter()\n",
        "number = random.randint(1,5)\n",
        "print(number)\n",
        "\n",
        "if number == 1:\n",
        "  print(\"Number is 1\")\n",
        "if number == 2:\n",
        "  print(\"Number is 2\")\n",
        "if number == 3:\n",
        "  print(\"Number is 3\")\n",
        "if number == 4:\n",
        "  print(\"Number is 4\")\n",
        "if number == 5:\n",
        "  print(\"Number is 5\")\n",
        "else:\n",
        "  print(\"No Number\")\n",
        "\n",
        "# Your code that you want to time\n",
        "end = time.perf_counter() - start\n",
        "print('{:.6f}s for the calculation'.format(end))\n"
      ],
